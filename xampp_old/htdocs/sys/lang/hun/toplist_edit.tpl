ction(){            //alert( jQuery(this).parent().html() );            jQuery(this).parent().parent().parent().parent().parent().clone().appendTo('#all');            jQuery(this).parent().parent().parent().parent().parent().remove();            return false;        });        jQuery('#mainForm').submit( function(){            var formValue = '';            jQuery('#top > li').each(function(){                formValue += jQuery(this).attr('id')+',';            })            jQuery('#toplist').val(formValue);        });    }); </script>					<script type="text/javascript">						Event.observe(window, 'load', function() {							new MaskedInput('#saxonsearch', 'a-9999?');						});					</script><div style="float:left;width:440px;">        <strong>ÖSSZES</strong><br/>        <i>aktív elemek amik nincsnenek a toplistában</i>        <br/>        <!--label>Keresés saxon szám szerint: <input type="text" id="search" value=""/>            <input type="button" value="OK" id="doSearch"/>        </label-->				            {$get_simple_search_toplist}        <br/>        <ul id="all" class="list" style="list-style:none;margin:0px;padding:0px;">            {$all}        </ul>    </div><form method="post" action="sys/process_toplist_edit.php" style="float:right; width:440px;" accept-charset="UTF-8" id="mainForm">    <div style="clear:both; width:100%; height:34px; text-align:center;">        <input type="submit" value="Mentés" /><br />        <input type="hidden" name="list" value="{$list}"/>    </div>    <div style="float:right;width:440px;">        <strong>TOPLISTA</strong><br/>        <br/>        <ol id="top" class="list" style="list-style:none;margin:0px;padding:0px;">            {$top}        </ol>    </div>    <input type="hidden" name="toplist" id="toplist" value=""/>    <div style="clear:both; width:100%; height:34px; text-align:center;">        <input type="submit" value="Mentés" /><br />        <a href="?page=admin">vi